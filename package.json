{
  "name": "transcript-parser",
  "version": "1.0.0",
  "description": "React TypeScript application",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon --watch server --exec tsx server/index.ts",
    "build": "tsc -b && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:web": "tsc -b && vite build",
    "preview": "vite preview",
    "preview:web": "vite preview --base=/transcript-parser/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "commit": "cz",
    "prepare": "husky",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "cross-env VITE_BASE_PATH=./ npm run build && electron-builder",
    "electron:build:win": "cross-env VITE_BASE_PATH=./ npm run build && cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder --win --x64",
    "electron:build:mac": "cross-env VITE_BASE_PATH=./ npm run build && electron-builder --mac",
    "electron:build:linux": "cross-env VITE_BASE_PATH=./ npm run build && electron-builder --linux"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@google/genai": "^1.34.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-virtual": "^3.13.13",
    "app-builder-bin": "^5.0.0-alpha.12",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "framer-motion": "^12.23.26",
    "idb": "^8.0.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.23",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.31.8",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.4",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "turbo": "^2.7.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "undici": "^7.16.0",
    "vite": "^6.0.5",
    "vite-plugin-pwa": "^1.2.0",
    "wait-on": "^9.0.3",
    "whatwg-fetch": "^3.6.20"
  },
  "keywords": [],
  "author": {
    "name": "Keven Markham",
    "email": "keven@example.com"
  },
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
